/*초기화*/
body, ul, h1, h2, h3, h4, h5, p, dl, dd { margin : 0; padding : 0; }
li { list-style : none; }
a { text-decoration : none; }
img { border : 0; }
address { font-style : normal; }
small { font-size : 1em; }
hr { display : none; }
.hidden { position : absolute; left : -9999px; }

/* 회색 */
body { background: #F6F6F6}

/* 폰트 */
@import url( "http://fonts.googleapis.com/earlyaccess/notosanskr.css" );

/* main header */
#main_header { width: 100%; height: 75px;
			 border: 1px solid #f44336;
			 background: #f44336;
			 z-index: 9999;
			 position: fixed;
			 }
			 
#header_wrap { width: 980px;
			position: relative;
			z-index: 9999px;
			margin: 0 auto;
			}

#link {
	position: absolute;
	top: 30px;
	right: 10px;
	z-index: 9999px;
}
#link ul li {
	float: left;
	margin-left: 9px;
}

.link_img {
	background: #fff;
	width: 40px; height: 40px;
	border-radius: 50%;
}

/* search box */
#search {
		position: absolute;
		right: 300px; top: 20px;}
.box {
	width: 412px;
	height:40px;
	border-style:solid; 
	border-width:1px;
	border-color:#ffffff;
	position: relative;
}

.search{
	font-size: 15px;
	padding: 0 15px;
	height: 100%;
	border: none;
	background: transparent;
	highlight-color: #ffffff;
	width: 300px;
	float: left;
	color: #fff;
}
.submit {
	position: absolute;
	font-size: 30px;
	padding: 0 15px;
	border: none;
	bottom: 3px;
}

textarea, input {
	font-family:"open sans", sans-serif;
}

.submit {
	font-weight: 500;
	background: transparent;
	color:#ffffff;
	height:100%;
	width:25%;
	float:right;
	transition: all 100ms ease-in-out;
	text-align: center;
	vertical-align: middle;
}

input::-webkit-input-placeholder { 
	color:#fff;
	font-family:"open sans", sans-serif;
	}
.placeholder::-webkit-input-placeholder {
	color:#c0c0c0;
	font-family:"open sans", sans-serif;
}

