/* 팔로워 리스트 */
.fade {float: right; width: 550px;
	 overflow: hidden;}

.fade .tabs {
	float: left; overflow: auto;
}

.fade .tabs li {
	float: left; 
	border: 1px solid #f44336;
	color: #ffffff;
    padding: 8px 20px;
	margin: 0px 7px 7px 7px;
	border-radius: 4px;
	background: white;
}

.fade .tabs li a {
	display: block; float: left; 
	width: 100px; height: 25px;
	text-align: center;
	color: #000;
}
.fade .tabs li:hover {
	border: 1px solid #0080ff;
}
.fade .tabs li a.selected {
	color: #f44336;	
}

.fade .items {
	clear: both;
	position: relative;
	top: 0; left: 0;
	height: 800px;
}
.fade .items div {
	width: 100%;
	position: absolute;
	height: 800px;
}

@import url(https://fonts.googleapis.com/css?family=Open+Sans:200);
@import url(http://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css);
.snip1578 {
  font-family: 'Open Sans', Arial, sans-serif;
  position: relative;
  display: inline-block;
  margin: 20px 8px;
  /* min-width: 230px;
  max-width: 315px;
  width: 100%; */
  width: 230px;
  color: #000;
  text-align: left;
  font-size: 16px;
  background: #fff;
  border-radius: 5px;
  border: 1px solid #ccc;
}

.snip1578 *,
.snip1578 {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-transition: all 0.4s ease;
  transition: all 0.4s ease;
}
.snip1578 img {
  width: 80px; height: 80px; border-radius: 50%;
  margin-top: -15px;
  margin-left: 60%;
  margin-bottom: 15px;
  backface-visibility: hidden;
  vertical-align: top;
}

.snip1578 figcaption {
  position: absolute;
  top: 0;
  right: 40%;
  left: 0;
  bottom: 0;
  padding: 15px;
}

.snip1578 h3 {
  margin: 0;
  font-size: 1.1em;
  font-weight: normal;
}

.snip1578 .icons {
  font-size: 1.6rem;
}

.snip1578 .icons a {
  color: #ccc;
}

.snip1578 .icons a:hover {
  color: #2980b9;
}

